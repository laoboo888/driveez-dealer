
{
    "version": 2,
    "builds": [
      {
        "src": "CarDealerWeb/cardealer/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "CarDealerWeb/staticfiles_build"
        }
      }
    ],
    "routes": [
        {
          "src": "CarDealerWeb/cardealer/static/(.*)",
          "dest": "CarDealerWeb/cardealer/static/$1"
        },
      {
        "src": "/(.*)",
        "dest": "CarDealerWeb/cardealer/wsgi.py"
      }
    ]
  }
